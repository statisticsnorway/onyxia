{
    "homepage": "https://onyxia.sh",
    "name": "onyxia-web",
    "version": "4.15.3",
    "license": "MIT",
    "scripts": {
        "_comment": "Yes... We're still using CRA. We'll migrate to Vite as soon as Keycloakify gets support for it",
        "start": "generate-env-getter && copy-keycloak-resources-to-public && ts-node scripts/unyamlify-env-local.ts && react-app-rewired start",
        "build": "react-app-rewired build && rimraf build/keycloak-resources",
        "build-keycloak-theme": "yarn build && keycloakify",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "yarn _format --write",
        "format:check": "yarn _format --list-different",
        "emails-domain-accept-list-helper": "ts-node scripts/emails-domain-accept-list-helper.ts",
        "prepare": "cp -rf patches/fs node_modules/ && patch-package && generate-env-getter && copy-material-icons-to-public"
    },
    "keycloakify": {
        "themeName": "onyxia",
        "extraThemeProperties": [
            "RESOURCES_ALLOWED_ORIGINS=${env.ONYXIA_RESOURCES_ALLOWED_ORIGINS:*}",
            "HEADER_TEXT_BOLD=${env.ONYXIA_HEADER_TEXT_BOLD:}",
            "HEADER_TEXT_FOCUS=${env.ONYXIA_HEADER_TEXT_FOCUS:}",
            "PALETTE_OVERRIDE=${env.ONYXIA_PALETTE_OVERRIDE:}",
            "TAB_TITLE=${env.ONYXIA_TAB_TITLE:}"
        ]
    },
    "dependencies": {
        "@duckdb/duckdb-wasm": "^1.13.1-dev230.0",
        "@duckdb/duckdb-wasm-shell": "^1.13.1-dev230.0",
        "@emotion/cache": "^11.11.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "5.14.15",
        "@mui/material": "^5.14.8",
        "@mui/x-data-grid": "^6.18.2",
        "@ungap/structured-clone": "^1.2.0",
        "async-mutex": "^0.4.0",
        "axios": "^0.26.0",
        "buffer": "^6.0.3",
        "color": "^4.2.3",
        "compare-versions": "^5.0.1",
        "cra-envs": "^2.1.2",
        "crypto-browserify": "^3.12.0",
        "evt": "^2.5.7",
        "file-saver": "^2.0.2",
        "filereader-stream": "^2.0.0",
        "flexsearch": "0.7.21",
        "https-browserify": "^1.0.0",
        "i18nifty": "^3.2.0",
        "json5": "^2.2.3",
        "keycloakify": "^9.4.1",
        "memoizee": "^0.4.14",
        "minimal-polyfills": "^2.2.3",
        "minio": "^7.1.1",
        "moment": "^2.29.1",
        "mustache": "^4.2.0",
        "oidc-spa": "^4.2.0",
        "onyxia-ui": "^5.1.3",
        "path": "^0.12.7",
        "path-browserify": "^1.0.1",
        "powerhooks": "^1.0.8",
        "process": "^0.11.10",
        "react": "^18.2.0",
        "react-code-blocks": "^0.0.9-0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "clean-architecture": "^4.3.1",
        "run-exclusive": "^2.2.19",
        "screen-scaler": "^1.2.7",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "tsafe": "^1.6.6",
        "tss-react": "^4.9.4",
        "type-route": "^1.1.0",
        "url-join-ts": "^1.0.5",
        "xterm": "^4.17.0",
        "xterm-addon-fit": "^0.5.0",
        "xterm-addon-web-links": "^0.5.1",
        "xterm-addon-webgl": "^0.11.4",
        "yaml": "^2.0.1",
        "zod": "^3.19.0",
        "timers-browserify": "^2.0.12",
        "url": "^0.11.3",
        "rimraf": "^5.0.5"
    },
    "devDependencies": {
        "@types/color": "^3.0.3",
        "@types/file-saver": "^2.0.2",
        "@types/flexsearch": "^0.7.3",
        "@types/memoizee": "^0.4.4",
        "@types/minio": "^7.0.13",
        "@types/mustache": "^4.2.2",
        "@types/node": "^14.0.13",
        "@types/react": "^18.0.19",
        "@types/react-dom": "^18.0.6",
        "@types/ungap__structured-clone": "0.3.0",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "circular-dependency-plugin": "^5.2.2",
        "dotenv": "^16.3.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-tss-unused-classes": "^1.0.2",
        "events": "^3.2.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "patch-package": "^8.0.0",
        "prettier": "^2.3.0",
        "react-app-rewired": "^2.2.1",
        "ts-node": "^10.2.1",
        "typescript": "^4.8.3"
    },
    "eslintConfig": {
        "plugins": [
            "tss-unused-classes"
        ],
        "extends": [
            "react-app",
            "prettier"
        ],
        "rules": {
            "@typescript-eslint/no-redeclare": "off",
            "import/no-anonymous-default-export": "off",
            "react-hooks/rules-of-hooks": "warn",
            "react-hooks/exhaustive-deps": "off",
            "no-labels": "off",
            "prefer-const": "off",
            "no-sequences": "off",
            "tss-unused-classes/unused-classes": "warn",
            "no-lone-blocks": "off"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
